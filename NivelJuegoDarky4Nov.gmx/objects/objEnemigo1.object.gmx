            <string>
if (global.pause) exit;

///Colisiones


hsp = dir * movespeed;
vsp += grav;

// Colisión horizontal
    
if (place_meeting(x+hsp,y, objBlock))
{
    while (!place_meeting(x+sign(hsp),y, objBlock))
    {
    x += sign(hsp);    
   } 
   
    hsp = 0;
    
    dir *= -1;
}

x += hsp;
  
  // Colisión vertical
    
if (place_meeting(x, y+vsp, objBlock)){
    while (!place_meeting(x, y+sign(vsp), objBlock)){
    y += sign(vsp);    
   } 
   vsp = 0;
}
//Colisión Ball
    
if (place_meeting(x,y,objBall))
{ 
    if (objBall.y &lt; y-16)  
    {
        with (objBall) vsp = -jumpspeed;
        instance_destroy ();
   } 
}         

y += vsp;

//Colisión Ball2
    
if (place_meeting(x,y,objBall2))
{ 
    if (objBall2.y &lt; y-16)  
    {
        with (objBall2) vsp = -jumpspeed;
        instance_destroy ();
   } 
}         



///Colisión objeto invisible

if (place_meeting(x+hsp,y, invi ))
{
    while (!place_meeting(x+sign(hsp),y, invi ))
    {
    x += sign(hsp);    
   } 
   
    hsp = 0;
    
    dir *= -1;
}

x += hsp;
  
  // Colisión vertical
    
if (place_meeting(x, y+vsp, invi )){
    while (!place_meeting(x, y+sign(vsp), invi )){
    y += sign(vsp);    
   } 
   vsp = 0;
}

y += vsp;



 
